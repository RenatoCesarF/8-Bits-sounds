{
    "name": "8bit-sounds",
    "publisher": "mattogodoy",
    "displayName": "8bit Sounds",
    "description": "This extension automatically turns you into a very skilled 8bit by playing movie-like sounds while you write code.",
    "version": "1.3.0",
    "engines": {
        "vscode": "^1.40.0"
    },
    "homepage": "https://matto.io",
    "repository": {
        "type": "git",
        "url": "https://github.com/mattogodoy/8bit-sounds"
    },
    "keywords": [
        "sound",
        "keyboard",
        "8bit",
        "fx",
        "sci-fi",
        "video-game",
        "videogame",
        "atari",
        "old vide game",
        "mario",
        "sounds",
        "key"
    ],
    "icon": "icon.png",
    "license": "MIT",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "8bit_sounds.enable",
                "title": "8bit Sounds: Enable"
            },
            {
                "command": "8bit_sounds.disable",
                "title": "8bit Sounds: Disable"
            },
            {
                "command": "8bit_sounds.volumeUp",
                "title": "8bit Sounds: Volume Up"
            },
            {
                "command": "8bit_sounds.volumeDown",
                "title": "8bit Sounds: Volume Down"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/lodash.debounce": "^4.0.6",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.40.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "tslint": "^5.20.0",
        "typescript": "^3.9.10",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {
        "lodash.debounce": "^4.0.8",
        "path": "^0.12.7",
        "play-sound": "^1.1.3"
    }
}
